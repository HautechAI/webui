{
    "name": "@hautechai/webui.visualeditorinput",
    "description": "Visual editor input component with keyframe support",
    "version": "2.1.7",
    "type": "module",
    "main": "src/index.tsx",
    "publishConfig": {
        "directory": "dist",
        "access": "public",
        "linkDirectory": false
    },
    "scripts": {
        "prepack": "node ../../scripts/build-component.mjs",
        "lint:eslint": "eslint src --ext .ts,.tsx",
        "lint:eslint:fix": "eslint src --ext .ts,.tsx --fix",
        "lint:tsc": "tsc --noEmit",
        "lint:prettier": "prettier --check src",
        "lint:prettier:fix": "prettier --write src",
        "lint": "pnpm lint:eslint && pnpm lint:tsc && pnpm lint:prettier",
        "lint:fix": "pnpm lint:eslint:fix && pnpm lint:prettier:fix"
    },
    "author": "HautechAI",
    "dependencies": {
        "@hautechai/webui.dropdown": "workspace:^",
        "@hautechai/webui.icon": "workspace:^",
        "@hautechai/webui.keyframetoggle": "workspace:^",
        "@hautechai/webui.textinput": "workspace:^",
        "@hautechai/webui.themeprovider": "workspace:^",
        "@hautechai/webui.toggleiconbutton": "workspace:^",
        "@hautechai/webui.typography": "workspace:^"
    },
    "license": "Apache-2.0"
}
